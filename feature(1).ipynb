      "source": [
        "import numpy as np\n",
        "\n",
        "def calculate_m_and_v(data):\n",
        "  m =np.mean(data)\n",
        "  v =np.var(data,ddof=1)\n",
        "  return m,v\n",
        "\n",
        "data =np.array([20,30,40,50,60])\n",
        "m,v =calculate_m_and_v(data)\n",
        "print(\"Mean value is:\",m)\n",
        "print(\"Variance value is:\",v)\n",
        "\n"
      ]
